<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.tt.mapper.DeviceMapper">
    
    <resultMap type="Device" id="DeviceResult">
        <result property="deviceId"    column="device_id"    />
        <result property="mechId"    column="mech_id"    />
        <result property="mechName"    column="mech_name"    />
        <result property="mechOwn"    column="mech_own"    />
        <result property="mechOwnName"    column="mech_own_name"    />
        <result property="deviceName"    column="device_name"    />
        <result property="deviceNo"    column="device_no"    />
        <result property="deviceStatus"    column="device_status"    />
        <result property="deviceStartTime"    column="device_start_time"    />
        <result property="deviceEndTime"    column="device_end_time"    />
        <result property="deviceMemo1"    column="device_memo1"    />
        <result property="deviceMemo2"    column="device_memo2"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="followNumber"    column="follow_number"    />
        <result property="followMinSpeed"    column="follow_min_speed"    />
        <result property="followMaxSpeed"    column="follow_max_speed"    />
        <result property="followSleepTime"    column="follow_sleep_time"    />
        <result property="followSleepCount"    column="follow_sleep_count"    />
        <result property="followMatchNickname"    column="follow_match_nickname"    />
        <result property="followMatchSignature"    column="follow_match_signature"    />
        <result property="followMatchComment"    column="follow_match_comment"    />
        <result property="followSex"    column="follow_sex"    />
        <result property="followMinAge"    column="follow_min_age"    />
        <result property="followMaxAge"    column="follow_max_age"    />
    </resultMap>

    <sql id="selectDeviceVo">
        select device_id, mech_id, mech_own,
            (select mech.mech_name from tt_mechant  mech where mech.mech_id = dev.mech_id) mech_name,
            (select mech.mech_name from tt_mechant  mech where mech.mech_id = dev.mech_own) mech_own_name,
               device_name, device_no, device_status, device_start_time, device_end_time,
               device_memo1, device_memo2, create_by, create_time, update_by, update_time,
               follow_number, follow_min_speed, follow_max_speed, follow_sleep_time, follow_sleep_count,
               follow_match_nickname, follow_match_signature, follow_match_comment, follow_sex,
               follow_min_age, follow_max_age from tt_device dev
    </sql>

    <select id="selectDeviceList" parameterType="Device" resultMap="DeviceResult">
        <include refid="selectDeviceVo"/>
        <where>
            <if test="mechId != null "> and mech_id = #{mechId}</if>
            <if test="mechOwn != null "> and mech_own = #{mechOwn}</if>
            <if test="deviceName != null  and deviceName != ''"> and device_name like concat('%', #{deviceName}, '%')</if>
            <if test="deviceNo != null  and deviceNo != ''"> and device_no = #{deviceNo}</if>
            <if test="deviceStatus != null  and deviceStatus != ''"> and device_status = #{deviceStatus}</if>
            <if test="deviceStartTime != null "> and device_start_time = #{deviceStartTime}</if>
            <if test="deviceEndTime != null "> and device_end_time = #{deviceEndTime}</if>
            <if test="deviceMemo1 != null  and deviceMemo1 != ''"> and device_memo1 = #{deviceMemo1}</if>
            <if test="deviceMemo2 != null  and deviceMemo2 != ''"> and device_memo2 = #{deviceMemo2}</if>
            <if test="followNumber != null "> and follow_number = #{followNumber}</if>
            <if test="followMinSpeed != null "> and follow_min_speed = #{followMinSpeed}</if>
            <if test="followMaxSpeed != null "> and follow_max_speed = #{followMaxSpeed}</if>
            <if test="followSleepTime != null "> and follow_sleep_time = #{followSleepTime}</if>
            <if test="followSleepCount != null "> and follow_sleep_count = #{followSleepCount}</if>
            <if test="followMatchNickname != null  and followMatchNickname != ''"> and follow_match_nickname like concat('%', #{followMatchNickname}, '%')</if>
            <if test="followMatchSignature != null  and followMatchSignature != ''"> and follow_match_signature = #{followMatchSignature}</if>
            <if test="followMatchComment != null  and followMatchComment != ''"> and follow_match_comment = #{followMatchComment}</if>
            <if test="followSex != null "> and follow_sex = #{followSex}</if>
            <if test="followMinAge != null "> and follow_min_age = #{followMinAge}</if>
            <if test="followMaxAge != null "> and follow_max_age = #{followMaxAge}</if>
        </where>
    </select>
    
    <select id="selectDeviceByDeviceId" parameterType="Long" resultMap="DeviceResult">
        <include refid="selectDeviceVo"/>
        where device_id = #{deviceId}
    </select>
    <select id="selectDeviceByDeviceNo" resultMap="DeviceResult">
        <include refid="selectDeviceVo"/>
        where device_no = #{deviceNo}
    </select>

    <insert id="insertDevice" parameterType="Device" useGeneratedKeys="true" keyProperty="deviceId">
        insert into tt_device
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="mechId != null">mech_id,</if>
            <if test="mechOwn != null">mech_own,</if>
            <if test="deviceName != null">device_name,</if>
            <if test="deviceNo != null">device_no,</if>
            <if test="deviceStatus != null">device_status,</if>
            <if test="deviceStartTime != null">device_start_time,</if>
            <if test="deviceEndTime != null">device_end_time,</if>
            <if test="deviceMemo1 != null">device_memo1,</if>
            <if test="deviceMemo2 != null">device_memo2,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="followNumber != null">follow_number,</if>
            <if test="followMinSpeed != null">follow_min_speed,</if>
            <if test="followMaxSpeed != null">follow_max_speed,</if>
            <if test="followSleepTime != null">follow_sleep_time,</if>
            <if test="followSleepCount != null">follow_sleep_count,</if>
            <if test="followMatchNickname != null">follow_match_nickname,</if>
            <if test="followMatchSignature != null">follow_match_signature,</if>
            <if test="followMatchComment != null">follow_match_comment,</if>
            <if test="followSex != null">follow_sex,</if>
            <if test="followMinAge != null">follow_min_age,</if>
            <if test="followMaxAge != null">follow_max_age,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="mechId != null">#{mechId},</if>
            <if test="mechOwn != null">#{mechOwn},</if>
            <if test="deviceName != null">#{deviceName},</if>
            <if test="deviceNo != null">#{deviceNo},</if>
            <if test="deviceStatus != null">#{deviceStatus},</if>
            <if test="deviceStartTime != null">#{deviceStartTime},</if>
            <if test="deviceEndTime != null">#{deviceEndTime},</if>
            <if test="deviceMemo1 != null">#{deviceMemo1},</if>
            <if test="deviceMemo2 != null">#{deviceMemo2},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="followNumber != null">#{followNumber},</if>
            <if test="followMinSpeed != null">#{followMinSpeed},</if>
            <if test="followMaxSpeed != null">#{followMaxSpeed},</if>
            <if test="followSleepTime != null">#{followSleepTime},</if>
            <if test="followSleepCount != null">#{followSleepCount},</if>
            <if test="followMatchNickname != null">#{followMatchNickname},</if>
            <if test="followMatchSignature != null">#{followMatchSignature},</if>
            <if test="followMatchComment != null">#{followMatchComment},</if>
            <if test="followSex != null">#{followSex},</if>
            <if test="followMinAge != null">#{followMinAge},</if>
            <if test="followMaxAge != null">#{followMaxAge},</if>
        </trim>
    </insert>

    <update id="updateDevice" parameterType="Device">
        update tt_device
        <trim prefix="SET" suffixOverrides=",">
            <if test="mechId != null">mech_id = #{mechId},</if>
            <if test="mechOwn != null">mech_own = #{mechOwn},</if>
            <if test="deviceName != null">device_name = #{deviceName},</if>
            <if test="deviceNo != null">device_no = #{deviceNo},</if>
            <if test="deviceStatus != null">device_status = #{deviceStatus},</if>
            <if test="deviceStartTime != null">device_start_time = #{deviceStartTime},</if>
            <if test="deviceEndTime != null">device_end_time = #{deviceEndTime},</if>
            <if test="deviceMemo1 != null">device_memo1 = #{deviceMemo1},</if>
            <if test="deviceMemo2 != null">device_memo2 = #{deviceMemo2},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="followNumber != null">follow_number = #{followNumber},</if>
            <if test="followMinSpeed != null">follow_min_speed = #{followMinSpeed},</if>
            <if test="followMaxSpeed != null">follow_max_speed = #{followMaxSpeed},</if>
            <if test="followSleepTime != null">follow_sleep_time = #{followSleepTime},</if>
            <if test="followSleepCount != null">follow_sleep_count = #{followSleepCount},</if>
            <if test="followMatchNickname != null">follow_match_nickname = #{followMatchNickname},</if>
            <if test="followMatchSignature != null">follow_match_signature = #{followMatchSignature},</if>
            <if test="followMatchComment != null">follow_match_comment = #{followMatchComment},</if>
            <if test="followSex != null">follow_sex = #{followSex},</if>
            <if test="followMinAge != null">follow_min_age = #{followMinAge},</if>
            <if test="followMaxAge != null">follow_max_age = #{followMaxAge},</if>
        </trim>
        where device_id = #{deviceId}
    </update>

    <delete id="deleteDeviceByDeviceId" parameterType="Long">
        delete from tt_device where device_id = #{deviceId}
    </delete>

    <delete id="deleteDeviceByDeviceIds" parameterType="String">
        delete from tt_device where device_id in 
        <foreach item="deviceId" collection="array" open="(" separator="," close=")">
            #{deviceId}
        </foreach>
    </delete>

</mapper>